{
  "name": "@su5kk/envchecker",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "envchecker": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc && chmod +x ./dist/index.js",
    "dev": "ts-node src/index.ts",
    "start": "npm run build && node dist/index.js",
    "test": "node --test test/integration.test.js",
    "test:watch": "node --test --watch test/integration.test.js"
  },
  "keywords": ["kubernetes", "environment-variables", "deployment", "validation", "devops", "cli"],
  "author": "su5kk",
  "license": "ISC",
  "description": "CLI tool to validate environment variables in Kubernetes deployments against reference files",
  "repository": {
    "type": "git",
    "url": "https://github.com/su5kk/envchecker.git"
  },
  "homepage": "https://github.com/su5kk/envchecker#readme",
  "bugs": {
    "url": "https://github.com/su5kk/envchecker/issues"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "zod": "^4.1.4"
  }
}
